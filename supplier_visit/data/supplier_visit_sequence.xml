<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Sequence for Supplier Visit -->
        <record id="seq_supplier_visit" model="ir.sequence">
            <field name="name">Supplier Visit</field>
            <field name="code">supplier.visit</field>
            <field name="prefix">VISIT/%(y)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo> 